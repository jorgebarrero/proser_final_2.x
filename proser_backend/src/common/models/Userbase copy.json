{
  "name": "Userbase",
  "base": "User",
  "idInjection": true,
  "properties": {
    "firstname": {
      "type": "string",
      "required": false
    },
    "lastname": {
      "type": "string",
      "required": false
    },
    "user_legal_id": {
      "type": "string",
      "required": false
    },
    "user_internal_id": {
      "type": "string",
      "required": false
    },
    "user_photo_path": {
      "type": "string",
      "required": false
    },
    "profile": {
      "type": "string",
      "required": false,
      "default": "user"
    },
    "profile_json": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },

    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "checkIfExists"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "accessType": ["READ", "WRITE"]
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": [
        "create",
        "deleteById",
        "findById",
        "find",
        "patchAttributes",
        "replaceById",
        "resetPassword",
        "changePassword"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "system",
      "permission": "ALLOW",
      "accessType": ["READ", "WRITE"]
    },
    {
      "principalType": "ROLE",
      "principalId": "system",
      "permission": "ALLOW",
      "property": [
        "create",
        "deleteById",
        "findById",
        "find",
        "patchAttributes",
        "replaceById",
        "resetPassword",
        "changePassword"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "develop",
      "permission": "ALLOW",
      "accessType": ["READ", "WRITE"]
    },
    {
      "principalType": "ROLE",
      "principalId": "develop",
      "permission": "ALLOW",
      "property": [
        "create",
        "deleteById",
        "findById",
        "find",
        "patchAttributes",
        "replaceById",
        "resetPassword",
        "changePassword"
      ]
    }
  ],
  "methods": {}
}
