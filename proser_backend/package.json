{
  "name": "proser-loopback",
  "version": "1.0.0",
  "main": "server/server.js",
  "scripts": {
    "start": "npx babel-node src/server/server.js",
    "start-dev": "nodemon --exec npx babel-node src/server/server.js",
    "backend": "nodemon --exec npx babel-node src/server/server.js",
    "system": "npx babel-node src/server-system/server.js",
    "users": "npx babel-node src/server-users/server.js",
    "build": "babel src --out-dir dist-backend",
    "start-prod": "npx node dist-backend/server/server.js",
    "load-day-prod": "node dist-backend/sync/etl/load/load_day.js",
    "pretest": "jshint .",
    "flush": "npx babel-node src/sync/flush/flush",
    "server": "npx babel-node src/server/server.js",
    "erase": "npx babel-node src/sync/etl/erase/erase.js",
    "erase-all": "npx babel-node src/sync/etl/erase/eraseAll.js",
    "load-inventory": "npx babel-node src/sync/etl/load/load_inventory.js",
    "load-hca": "npx babel-node src/sync/etl/load/load_hca.js",
    "load-main": "npx babel-node src/sync/etl/load/load_main.js",
    "load-real": "npx babel-node src/sync/etl/load/load_real.js",
    "load-day": "npx babel-node src/sync/etl/load/load_day.js",
    "history": "npx babel-node src/sync/etl/load/load_history.js",
    "duplicate-hca": "npx babel-node src/sync/etl/load/load_hca_duplicate.js",
    "bash": "bash sync/bash/getJSON.sh",
    "start-up": "npx babel-node src/sync/app",
    "start-import": "npx babel-node src/sync/imports/main_import",
    "start-update": "npx babel-node src/sync/updates/main_update",
    "start-realtime": "npx babel-node src/sync/realtime/main_realtime",
    "start-forever": "forever start -c 'babel-node src/sync/app.js'",
    "end-forever": "forever stop 'babel-node src/sync/app.js'",
    "test": "mocha test/*test.js",
    "update": "npx babel-node src/sync/etl/load/load_day.js",
    "cluster": "npx babel-node src/sync/cluster.js"
  },
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.5.5",
    "body-parser": "^1.10.1",
    "chalk": "^2.4.2",
    "colors": "^1.3.3",
    "compression": "^1.7.4",
    "cors": "^2.8.1",
    "dotenv": "^7.0.0",
    "eslint": "^6.1.0",
    "event-stream": "^4.0.1",
    "express-handlebars": "^3.0.2",
    "handlebars": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "loopback": "^3.25.1",
    "loopback-boot": "^2.28.0",
    "loopback-connector-mysql": "^5.3.1",
    "moment": "^2.24.0",
    "ng-pick-datetime-moment": "^1.0.8",
    "ngx-bootstrap": "^5.1.1",
    "node-http-ping": "^0.3.1",
    "nodemailer": "^6.2.1",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "rxjs": "^6.4.0",
    "serve-favicon": "^2.0.1",
    "set-interval-async": "1.0.26",
    "shelljs": "^0.8.3",
    "strong-error-handler": "^3.2.0",
    "supertest": "^3.4.2"
  },
  "optionalDependencies": {
    "loopback-component-explorer": "^2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/register": "^7.4.0",
    "eslint": "^6.1.0",
    "jshint": "^2.10.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.11",
    "prettier-eslint": "^8.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/strongloop/loopback-example-access-control"
  },
  "license": "MIT"
}
